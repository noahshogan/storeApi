CREATE TABLE Users (
id INT IDENTITY  PRIMARY KEY,
firstname NCHAR(20) NOT NULL,
lastname NCHAR(20) NOT NULL,
username NCHAR(20) NULL,
pword NCHAR(20) NOT NULL,
email NCHAR(20) NOT NULL,
    [managerlvl] INT NOT NULL DEFAULT 0
)